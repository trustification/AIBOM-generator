{
  "name": "aibom-generator-hf",
  "version": "0.1.0",
  "description": "A project to generate AIBOMs for the top trending models from Hugging Face.",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "pretest": "eslint . --ext .ts",
    "test": "node --import tsx --test test/test.ts",
    "coverage": "nyc --reporter=text --reporter=html --reporter=cobertura npm run test"
  },
  "dependencies": {
    "axios": "^1.12.2"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.0",
    "nyc": "^17.1.0",
    "ts-node": "^10.4.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trustification/aibom-generator.git"
  },
  "keywords": [
    "AIBOM",
    "Hugging Face",
    "model generator"
  ],
  "author": "Petr Toman",
  "license": "Apache-2.0"
}
